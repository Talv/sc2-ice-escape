        <Frame type="Frame" name="Ranking" template="IBE_Ranking/Ranking">
            <Anchor side="Top" relative="$parent/StatFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/StatFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/StatFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/StatFrame" pos="Max" offset="0"/>
            <Visible val="False" />
        </Frame>

        <Frame type="Frame" name="Options" template="IBE_Options/Options">
            <Anchor side="Top" relative="$parent/StatFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/StatFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/StatFrame" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent/StatFrame" pos="Max" offset="0"/>
            <Visible val="False" />

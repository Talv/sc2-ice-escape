bool libsefr_gf_sfr_is_admin (int lp_player) {
    // Variable Declarations
    string lv_handle;

    // Automatic Variable Declarations
    // Variable Initialization
    lv_handle = PlayerHandle(lp_player);

    // Implementation
    if (((GameIsTestMap(false) == true) || (lv_handle == "1-S2-1-4182020") || (lv_handle == "2-S2-1-2642502") || (lv_handle == "3-S2-1-4137301") || (lv_handle == "1-S2-1-4293706") || (lv_handle == "2-S2-1-1867792") || (lv_handle == "1-S2-1-4293706") || (lv_handle == "2-S2-1-1867792") || (lv_handle == "3-S2-1-4180450") || (lv_handle == "6-S2-1-532930") || (lv_handle == "3-S2-1-4180450"))) {
        return true;
    }
    else {
        return false;
    }
}

